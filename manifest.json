{
    "manifest_version": 3,
    "name": "LinkedIn Job Scraper",
    "version": "1.0",
    "description": "Scrapes job listings from LinkedIn jobs page",
    "permissions": [
        "activeTab",
        "scripting"
    ],
    "action": {
        "default_title": "LinkedIn Job Scraper"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.linkedin.com/jobs/*"
            ],
            "js": [
                "content.js"
            ]
        }
    ]
}